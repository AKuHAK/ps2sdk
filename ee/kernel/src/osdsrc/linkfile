ENTRY(_start);

SECTIONS {
	.text 0x80074000: {
		_ftext = . ;
		*(.start)
		*(.text)
		*(.text.*)
		*(.gnu.linkonce.t*)
		QUAD(0)
	}

	/* Static data.  */
	.rodata ALIGN(64): {
		*(.rodata)
		*(.rodata.*)
		*(.gnu.linkonce.r*)
	}

}

